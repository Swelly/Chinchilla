pkill -f "$1 $2"                      # kill exiting servers of the same type on this machine
git pull origin master                # update code
go build src/chinchilla/routers/$1.go # build startup script
if [ "$1" = "scheduler" ] || [ "$1" = "webserver" ] 
then
    ./$1 $2 $3
else 
    for i in $(seq 1 $3)
    do 
        ./$1 $2 $i &
        echo "starting worker $i"
    done
fi